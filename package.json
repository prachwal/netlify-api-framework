{
  "name": "netlify-api-framework",
  "version": "1.0.0",
  "description": "Modern API framework dedicated to Netlify Functions (TypeScript, Fetch API)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "test:framework:coverage": "npx vitest run --coverage --config vitest.config.netlify.ts",
    "serve:functions": "npx netlify functions:serve netlify/functions"
  },
  "keywords": [
    "netlify",
    "serverless",
    "typescript",
    "api",
    "framework",
    "fetch"
  ],
  "author": "Netlify",
  "license": "MIT",
  "dependencies": {
    "@netlify/functions": "4.1.6"
  },
  "devDependencies": {
    "typescript": "5.8.3",
    "@types/node": "24.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "tsx": "^4.20.3",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prachwal/app-netlify-05.git"
  }
}
